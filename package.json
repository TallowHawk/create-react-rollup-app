    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-visualizer": "^0.9.0",
    "serve": "^10.0.1",
    "systemjs": "^0.21.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "react-test-renderer": "^16.5.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "eslint --fix --ext .jsx,.js",
      "git add"
